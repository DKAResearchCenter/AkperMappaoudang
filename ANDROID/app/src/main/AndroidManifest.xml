<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dka.product.akper">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Bhayangkara">
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity
            android:name="dka.product.akper.splashScreen"
            android:theme="@style/Theme.Bhayangkara">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="dka.product.akper.MainActivity"
            android:theme="@style/Theme.Bhayangkara" />

        <activity
            android:name="dka.product.akper.view.Features.pmbOnline"
            android:theme="@style/Theme.Bhayangkara" />

        <activity
            android:name="dka.product.akper.view.Features.cctvActivity"
            android:theme="@style/Theme.Bhayangkara" />

    </application>

</manifest>